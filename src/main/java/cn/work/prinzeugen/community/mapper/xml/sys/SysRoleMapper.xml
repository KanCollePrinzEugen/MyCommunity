<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.work.prinzeugen.community.mapper.sys.SysRoleMapper">
    <select id="findUserRole" parameterType="Integer" resultType="cn.work.prinzeugen.community.entity.sys.SysRole">
    SELECT * FROM super_mall.sys_role WHERE id IN (SELECT role_id FROM super_mall.sys_admin_role WHERE admin_id = ${userId})
    </select>
</mapper>